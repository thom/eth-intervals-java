#!/bin/bash

MYDIR=$(dirname $0)
DIR=$(cd $MYDIR; pwd)
$DIR/benchmark-init $1

for iter in 500 1000 1500
do
	echo Testing 8 threads with $iter iterations
	for N in 1 2 3 4 5
	do 
		java ${JAVA_OPTS} erco.sor.threads.Sor $iter 8
	done

	echo Testing intervals with $iter iterations
	for N in 1 2 3 4 5
	do 
		java ${JAVA_OPTS} erco.sor.intervals.Sor $iter
	done
done
