#!/bin/bash

COUNT=$1
RE=$2/re

if [[ -s $COUNT ]]
then
	echo -e "\nWork steals:"
	echo -e "Steal attempt\tUnsuccessful\tSuccessful"
	echo -e "$($RE 'WORKER_STEAL ATTEMPT' 'hit' < $COUNT | wc -l)\t\t$($RE 'WORKER_STEAL UNSUCCESSFUL' 'hit' < $COUNT | wc -l)\t\t$($RE 'WORKER_STEAL SUCCESSFUL' 'hit' < $COUNT | wc -l)\n"
else
	echo # new line
fi
